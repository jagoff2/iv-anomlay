{
  "kafka": {
    "bootstrap_servers": "redpanda:9092",
    "topic": "options_data",
    "batch_size": 100,
    "queue_buffer_size": 10000,
    "linger_ms": 100,
    "compression_type": "lz4"
  },
  "yfinance": {
    "tickers": ["AAPL", "MSFT", "GOOGL", "AMZN", "META", "TSLA", "SPY", "QQQ", "IWM", "XOM", "PLTR"],
    "fetch_interval": 60,
    "max_expiry_days": 90,
    "risk_free_rate": 0.05
  },
  "general": {
    "debug": false,
    "num_workers": 4
  }
}
